{
  "database": {
    "connectionString": "Server=localhost;Database=halo-sqldb;User Id=sa;Password=YourPassword;TrustServerCertificate=True;Encrypt=True;",
    "schemas": {
      "source": "new_data",
      "transition": "raw_data",
      "processing": "etl",
      "production": "dbo"
    }
  },
  "inputFiles": {
    "premiums": "/path/to/premiums.csv",
    "certificateInfo": "/path/to/CertificateInfo.csv",
    "commissionsDetail": "/path/to/CommissionsDetail_*.csv",
    "individualBrokers": "/path/to/IndividualRosterExtract_*.csv",
    "orgBrokers": "/path/to/OrganizationRosterExtract_*.csv",
    "licenses": "/path/to/BrokerLicenseExtract_*.csv",
    "eo": "/path/to/BrokerEO_*.csv",
    "scheduleRates": "/path/to/perf.csv",
    "perfGroups": "/path/to/perf-group.csv",
    "fees": "/path/to/Fees_*.csv"
  },
  "debugMode": {
    "enabled": false,
    "maxRecords": {
      "brokers": 100,
      "groups": 50,
      "policies": 1000,
      "premiums": 5000,
      "hierarchies": 100,
      "proposals": 50
    }
  },
  "resume": {
    "enabled": false,
    "resumeFromRunId": null
  },
  "ProposalBuilderV2": {
    "highEntropyUniqueRatio": 0.20,
    "highEntropyShannon": 5.0,
    "dominantCoverageThreshold": 0.25,
    "phaClusterSizeThreshold": 3,
    "smallFragmentedCertThreshold": 15,
    "smallFragmentedConfigThreshold": 2,
    "logEntropyByGroup": false
  }
}
