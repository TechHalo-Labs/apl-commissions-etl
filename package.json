{
  "name": "v5-etl",
  "version": "1.0.0",
  "description": "SQL Server ETL Pipeline for APL Commissions",
  "main": "scripts/run-pipeline.ts",
  "scripts": {
    "pipeline": "npx tsx scripts/run-pipeline.ts",
    "pipeline:ts": "npx tsx scripts/run-pipeline.ts --use-ts-builder",
    "build-proposals": "npx tsx scripts/proposal-builder.ts --mode transform",
    "build-proposals:dry": "npx tsx scripts/proposal-builder.ts --mode transform --dry-run --verbose",
    "build-proposals:export": "npx tsx scripts/proposal-builder.ts --mode export",
    "build-proposals:export-dry": "npx tsx scripts/proposal-builder.ts --mode export --dry-run",
    "build-proposals:full": "npx tsx scripts/proposal-builder.ts --mode full",
    "build-proposals:full-dry": "npx tsx scripts/proposal-builder.ts --mode full --dry-run --verbose",
    "validate-certificates": "npx tsx scripts/validate-certificate-resolution.ts",
    "ingest": "npx tsx scripts/ingest-csv.ts",
    "transform": "npx tsx scripts/run-transforms.ts",
    "calculate": "npx tsx scripts/run-calculation.ts",
    "export": "npx tsx scripts/export-to-dbo.ts",
    "populate-special-rates": "npx tsx scripts/populate-special-schedule-rates.ts",
    "populate-special-rates:dry": "npx tsx scripts/populate-special-schedule-rates.ts --dry-run",
    "test": "npx tsx scripts/test-pipeline.ts",
    "test:consolidation": "npx tsx scripts/transforms/__tests__/consolidate-proposals.test.ts",
    "verify:consolidation": "npx tsx scripts/verify-consolidation.ts"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.30.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.3",
    "mssql": "^10.0.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.10.0",
    "@types/yauzl": "^2.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
